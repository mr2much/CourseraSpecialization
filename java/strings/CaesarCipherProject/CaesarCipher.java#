public class CaesarCipher {
    private final static String ALPHABET = "ABCDEFGHIJKLMNOPQRSTUVWXYZ";
    int key;
    
    public CaesarCipher(int key) {
        this.key = key;
    }
    
    public String getAlphabet() {
        return "RSTUVWXYZABCDEFGHIJKLMNOPQ";
    }
}
